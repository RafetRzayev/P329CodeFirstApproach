@model SliderBg
<div class="row">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Create slider bg</h4>
                <form class="forms-sample" method="post" enctype="multipart/form-data">
                    <div class="form-group">
                        <label asp-for="Photo">Name</label>
                        <input asp-for="Photo" class="form-control" placeholder="Username" accept="image/*">
                        <span asp-validation-for="Photo"></span>
                    </div>
                    <button type="submit" class="btn btn-primary mr-2">Submit</button>
                    <button class="btn btn-light" asp-controller="index">Cancel</button>
                </form>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <partial name="_CkeEditorPartial"></partial>
}